{# _composant-menu-responsive.html.twig #}

<div class="inline-block px-[6vw]">
    <div class="menu-item" @click="toggleMenu">
        <img src="{{ default_img }}" class="img-responsive w-[40px] h-[40px] m-auto" alt="Image illustrant le menu {{ menu_name }}" />
        <p class="text-center">{{ menu_name }} </p>
    </div>
</div>



